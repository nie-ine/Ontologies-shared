<http://www.knora.org/ontology/shared/linguistics>
	a owl:Ontology;
	dct:license <http://creativecommons.org/licenses/by/3.0/>;
	dct:title "An ontology about language"@en;
	dct:description """Formal description of linguistics, general subclasses, related classes and properties."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:contributor "Reto Baumgartner, tool coordinator and software developer, NIE-INE, University of Z체rich, Switzerland"@en;
	dct:contributor "Magdalena Plamada, PhD, Z체rich University, Switzerland"@en;
	dct:publisher "Basel University, Switzerland"@en;

	knora-base:attachedToProject <http://www.knora.org/ontology/knora-base#DefaultSharedOntologiesProject>.

#
# CLASSES
#

ling:Concept
	a owl:Class;
	rdfs:label "linguistic concept"@en, "linguistisches Konzept"@de;
	rdfs:comment """Concept in the knowledge domain of linguistics."""@en;
	rdfs:subClassOf concept:Concept, [
		a owl:Restriction; owl:onProperty ling:hasLanguage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty ling:hasLanguageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

ling:Lexeme 
	a owl:Class;
	rdfs:label "lexeme"@en, "Lexem"@de;
	rdfs:comment """Linguistic concept as individual semantic entity in human natural language."""@en;
	rdfs:subClassOf ling:Concept, language:HumanNaturalLanguageExpression, [
#		a owl:Restriction; owl:onProperty ling:hasSemanticGender; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty ling:hasSemanticGenderValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty ling:hasLemma; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty ling:hasLemmaValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty ling:hasPartOfSpeech; owl:minCardinality "0"^^xsd:nonNegativeInteger ], [
		a owl:Restriction; owl:onProperty ling:hasPartOfSpeechValue; owl:minCardinality "0"^^xsd:nonNegativeInteger ].

ling:Lemma
	a owl:Class;
	rdfs:label "lemma"@en, "Lemma"@de;
	rdfs:comment """Dictionary entry of a lexeme."""@en;
	rdfs:seeAlso <http://www.alphabit.net/ATL3Leunitadianalisi.pdf>, <http://www.dcuci.univr.it/documenti/OccorrenzaIns/matdid/matdid121824.pdf>;
	rdfs:subClassOf ling:Concept, concept:SymbolicConcept.

ling:Symbol
	a owl:Class;
	rdfs:label "language symbol"@en, "Sprache-Symbol"@de;
	rdfs:comment """."""@en;
	rdfs:subClassOf text:WrittenText.

ling:Literal
	a owl:Class;
	rdfs:label "literal"@en, "Literal"@de;
	rdfs:comment """One or more language symbols of any kind."""@en;
	skos:note """Not only letters; as also used in Information Technology, e.g. Resource Description Framework."""@en;
	rdfs:subClassOf text:WrittenText.

ling:PartOfSpeech
	a owl:Class;
	rdfs:label "part of speech"@en, "Wortart"@de;
	rdfs:comment """Word with certain grammatical properties and syntactic role in the structure of a sentence."""@en;
	rdfs:subClassOf ling:Concept.

ling:Noun # owl:sameAs gold:Noun, EAGLES: Noun
	a owl:Class;
	rdfs:label "noun"@en;
	rdfs:comment """Part of speech representing a class or instance."""@en;
	skos:note """In German there is no grouping concept for 'Substantiv' and 'Pronomen'."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

ling:CommonNoun # EAGLES: Noun: Common
	a owl:Class;
	rdfs:label "common noun"@en, "Substantiv"@de;
	rdfs:comment """Part of speech representing a concept or class."""@en;
	skos:note """In German written with starting capital."""@en;
	rdfs:subClassOf ling:Noun.

ling:ProperNoun # EAGLES: Noun: Proper
	a owl:Class;
	rdfs:label "proper noun"@en, "Eigenname"@de;
	rdfs:comment """Part of speech representing an instance."""@en;
	skos:note """Written with starting capital, for naming, e.g. a person, a geographical entity (e.g. a country); in English and German also a calendar entity (e.g. resp. 'Monday', 'Montag')"""@en;
	rdfs:subClassOf ling:Noun.

ling:Verb # EAGLES: Verb
	a owl:Class;
	rdfs:label "verb"@en, "Verb"@de;
	rdfs:comment """Part of speech governing a sentences by introducing existence, change, action, and linking a subject and object."""@en;
	rdfs:subClassOf ling:PartOfSpeech.
	
ling:Adjective	# EAGLES: Adjective
	a owl:Class;
	rdfs:label "adjective"@en, "Adjektiv"@de;
	rdfs:comment """Part of speech describing an object."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

ling:Pronoun # EAGLES: Pronoun/Determiner
	a owl:Class; 
	rdfs:label "pronoun"@en, "Pronomen"@de; # determiner, Begleiter
	rdfs:comment """Part of speech quantifying or replacing an object."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

lingmor:DemonstrativePronoun
	a owl:Class;
	rdfs:label "demonstrative pronoun"@en, "Demonstrativpronomen"@de;
	rdfs:comment """Pronoun used to point."""@en;
	rdfs:subClassOf ling:Pronoun.

ling:IndefinitePronoun
	a owl:Class;
	rdfs:label "indefinite pronoun"@en, "Indefinitpronomen"@de;
	rdfs:comment """Pronoun stating an undefined entity."""@en;
	rdfs:subClassOf ling:Pronoun.

ling:InterrogativePronoun
	a owl:Class;
	rdfs:label "interrogative pronoun"@en, "Interrogativpronomen"@de;
	rdfs:comment """Pronoun introducing a question."""@en;
	rdfs:subClassOf ling:Pronoun.
	
ling:PersonalPronoun
	a owl:Class;
	rdfs:label "personal pronoun"@en, "Personalpronomen"@de;
	rdfs:comment """Pronoun referring to a person, and declined by person, gender, number, case and formality."""@en;
	rdfs:subClassOf ling:Pronoun.

ling:ReciprocalPronoun
	a owl:Class;
	rdfs:label "reciprocal pronoun"@en, "Reziprokpronomen"@de;
	rdfs:comment """Pronoun pointing to the different members of the group as subject of the sentence, e.g. 'each other'."""@en;
	rdfs:subClassOf ling:Pronoun.
	
ling:ReflexivePronoun
	a owl:Class;
	rdfs:label "reflexive pronoun"@en, "Reflexivpronomen"@de;
	rdfs:comment """Pronoun pointing back to the subject of the sentence, e.g. 'yourself'."""@en;
	rdfs:subClassOf ling:Pronoun.
	
ling:RelativePronoun
	a owl:Class;
	rdfs:label "relative pronoun"@en, "Relativpronomen"@de;
	rdfs:comment """Pronoun introducing a relative clause."""@en;
	rdfs:subClassOf ling:Pronoun.

ling:Article # EAGLES: Article
	a owl:Class;
	rdfs:label "article"@en, "Artikel"@de;
	rdfs:comment """Part of speech specifying grammatical definiteness, i.e. indicating if an object is known or newly introduced."""@en;
	rdfs:subClassOf ling:PartOfSpeech.
	
ling:DefiniteArticle # EAGLES: Article: Definite
	a owl:Class;
	rdfs:label "definite article"@en, "bestimmter Artikel"@de;
	rdfs:comment """Article indicating that an object is known."""@en;
	rdfs:subClassOf ling:Article.
	
ling:IndefiniteArticle # EAGLES: Article: Indefinite
	a owl:Class;
	rdfs:label "indefinite article"@en, "unbestimmter Artikel"@de;
	rdfs:comment """Article indicating that an object is newly introduced."""@en;
	rdfs:subClassOf ling:Article.

ling:Adverb # EAGLES: Adverb 
	a owl:Class;
	rdfs:label "adverb"@en, "Adverb"@de;
	rdfs:comment """Part of speech modifying a verb (e.g. describing an action), adjective, pronoun, clause, preposition, or sentence."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

ling:Adposition # EAGLES: Adposition 
	a owl:Class;
	rdfs:label "adposition"@en, "Adposition"@de;
	rdfs:comment """Part of speech indicating a relationship to objects."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

ling:Preposition # EAGLES: Adposition: Preposition
	a owl:Class;
	rdfs:label "preposition"@en, "Pr채position"@de;
	rdfs:comment """Part of speech indicating relationship to objects, standing before them."""@en;
	rdfs:subClassOf ling:Adposition.
	
ling:Postposition # EAGLES: Adposition: Postposition
	a owl:Class;
	rdfs:label "postposition"@en, "Postposition"@de;
	rdfs:comment """Part of speech indicating relationship to objects, standing after them."""@en;
	rdfs:subClassOf ling:Adposition.

ling:Conjunction #  EAGLES: Conjunction
	a owl:Class; 
	rdfs:label "conjunction"@en, "Konjunktion"@de;
	rdfs:comment """Part of speech linking syntactic entities."""@en;
	rdfs:subClassOf ling:PartOfSpeech.
	
ling:CoordinatingConjunction #  EAGLES: Conjunction: Coordinating
	a owl:Class;
	rdfs:label "coordinating conjunction"@en, "nebenordnende Konjunktion"@de;
	rdfs:comment """Part of speech that links syntactic entities on the same level."""@en;
	rdfs:subClassOf ling:Conjunction.
	
ling:SubordinatingConjunction #  EAGLES: Conjunction: Subordinating
	a owl:Class;
	rdfs:label "subordinating conjunction"@en, "unterordnende Konjunktion, Subjunktion"@de;
	rdfs:comment """Part of speech that orders one syntactic entity under another."""@en;
	rdfs:subClassOf ling:Conjunction.

ling:Numeral
	a owl:Class;
	rdfs:label "numeral"@en, "Numerale"@de;
	rdfs:comment """Part of speech designating a number."""@en;
	rdfs:subClassOf ling:PartOfSpeech.
	
ling:CardinalNumeral
	a owl:Class;
	#  EAGLES: Numeral: Cardinal 
	rdfs:label "cardinal numeral"@en, "Kardinalzahl"@de;
	rdfs:comment """Part of speech describing numbers as a measure."""@en;
	rdfs:subClassOf ling:Numeral.
	
ling:OrdinalNumeral
	a owl:Class;
	#  EAGLES: Numeral: Ordinal 
	rdfs:label "ordinal numeral"@en, "Ordinalzahl"@de;
	rdfs:comment """Part of speech describing numbers as an order."""@en;
	rdfs:subClassOf ling:Numeral.
	
ling:Interjection
	a owl:Class;
	# EAGLES: Interjection
	rdfs:label "interjection"@en, "Interjektion"@de;
	rdfs:comment """Part of speech designating a spontaneous feeling or reaction."""@en;
	rdfs:subClassOf ling:PartOfSpeech.
	
ling:NegativeParticle
	a owl:Class;
	# EAGLES: Unique/unassigned
	rdfs:label "negative particle"@en, "Negationspartikel"@de;
	rdfs:comment """Part of speech designating non traditionally accepted parts of speech with strong grammatical function."""@en;
	rdfs:subClassOf ling:PartOfSpeech.
	
ling:ExistentialParticle
	a owl:Class;
	# EAGLES: Unique/unassigned
	rdfs:label "existential particle"@en, "..."@de;
	rdfs:comment """Part of speech designating non traditionally accepted parts of speech with strong grammatical function."""@en;
	rdfs:subClassOf ling:PartOfSpeech.
	
ling:InfinitiveMarker
	a owl:Class;
	# EAGLES: Unique/unassigned
	rdfs:label "infinitive marker"@en, "Infinitivpartikel"@de;
	rdfs:comment """Part of speech designating non traditionally accepted parts of speech with strong grammatical function."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

ling:Residual
	a owl:Class;
	# EAGLES: Residual
	rdfs:label "residual"@en, "Residual"@de;
	rdfs:comment """Part of speech outside of the traditionally accepted range."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

ling:Punctuation
	a owl:Class;
	rdfs:label "punctuation"@en, "Interpunktion"@de;
	rdfs:comment """Part of speech describing the non letter or number based parts of a sentence."""@en;
	rdfs:subClassOf ling:PartOfSpeech.

# speech acts

ling:Statement
	a owl:Class;
	rdfs:label "statement"@en, "Aussage"@de;
	rdfs:comment """Sentence... ."""@en;
	rdfs:subClassOf text-structure:Sentence.

ling:Command
	a owl:Class;
	rdfs:label "command"@en, "Befehl"@de;
	rdfs:comment """Sentence... ."""@en;
	rdfs:subClassOf text-structure:Sentence.

ling:Question
	a owl:Class;
	rdfs:label "question"@en, "Frage"@de;
	rdfs:comment """Sentence eliciting information."""@en;
	rdfs:subClassOf text-structure:Sentence.

# request

ling:Answer
	a owl:Class;
	rdfs:label "answer"@en, "Antwort"@de;
	rdfs:comment """Sentence providing information on elicitation."""@en;
	rdfs:subClassOf text-structure:Sentence.

ling:Promise
	a owl:Class;
	rdfs:label "promise"@en, "Versprechen"@de;
	rdfs:comment """Sentence... ."""@en;
	rdfs:subClassOf text-structure:Sentence.

ling:Evaluation
	a owl:Class;
	rdfs:label "evaluation"@en, "Evaluation"@de;
	rdfs:comment """Examination with judgement of any kind, as output of evaluating."""@en;
	rdfs:subClassOf text-structure:Sentence.

#
# PROPERTIES
#

ling:hasLanguage
	a owl:ObjectProperty;
	rdfs:label "has language"@en, "hat Sprache"@de;
	rdfs:comment "Relating a linguistic concept to its human natural language."@en;
	rdfs:domain ling:Concept;
	rdfs:range language:HumanNaturalLanguage;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint ling:Concept;
	knora-base:objectClassConstraint language:HumanNaturalLanguage.

ling:hasLanguageValue
	a owl:ObjectProperty;
	rdfs:label "has language - statement"@en, "hat Sprache - Festlegung"@de;
	rdfs:comment "Relating a linguistic concept to a reification statement of the relation between the concept and its human natural language."@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint ling:Concept;
	knora-base:objectClassConstraint knora-base:LinkValue.

ling:hasComment
	a owl:ObjectProperty;
	rdfs:label "has linguistic comment"@en, "hat linguistischer Kommentar"@de;
	rdfs:comment """Relating a human natural language expression to a linguistic comment as a textual statement about the expression."""@en;
	rdfs:domain language:HumanNaturalLanguageExpression;
	rdfs:range rdfs:Literal;
	rdfs:subPropertyOf text:hasComment;
	knora-base:subjectClassConstraint language:HumanNaturalLanguageExpression;
	knora-base:objectClassConstraint knora-base:TextValue.

ling:hasSynonym
	a owl:ObjectProperty;
	rdfs:label "has synonym"@en, "hat Synonym"@de;
	rdfs:comment """Relating a resource to a synonym of the name of the resource."""@en;
	rdfs:domain rdfs:Resource; #째
	rdfs:range rdfs:Literal;
	rdfs:subPropertyOf text:hasName;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue.

