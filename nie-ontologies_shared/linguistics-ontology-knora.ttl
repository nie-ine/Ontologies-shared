ling:hasLemma
	a owl:ObjectProperty;
	rdfs:label "has lemma"@en, "hat Lemma"@de;
	rdfs:comment """Relating a lexeme to a lemma."""@en;
	rdfs:domain ling:Lexeme;
	rdfs:range ling:Lemma;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint ling:Lexeme;
	knora-base:objectClassConstraint ling:Lemma.

ling:hasLemmaValue
	a owl:ObjectProperty;
	rdfs:label "has lemma - statement"@en, "hat Lemma - Festlegung"@de;
	rdfs:comment """Relating a lexeme to a reification statement of the relation between the lexeme and a lemma."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint ling:Lexeme;
	knora-base:objectClassConstraint knora-base:LinkValue.

ling:hasPartOfSpeech
	a owl:ObjectProperty;
	rdfs:label "has part of speech"@en, "hat Wortart"@de;
	rdfs:comment """Relating a lexeme to its part of speech."""@en;
	rdfs:domain ling:Lexeme;
	rdfs:range ling:PartOfSpeech;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint ling:Lexeme;
	knora-base:objectClassConstraint ling:PartOfSpeech.

ling:hasPartOfSpeechValue
	a owl:ObjectProperty;
	rdfs:label "has part of speech - statement"@en, "hat Wortart - Festlegung"@de;
	rdfs:comment """Relating a lexeme to a reification statement of the relation between the lexeme and its part of speech."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint ling:Lexeme;
	knora-base:objectClassConstraint knora-base:LinkValue.