@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix event: <http://www.knora.org/ontology/shared/event#>.
@prefix human: <http://www.knora.org/ontology/shared/human#>.
@prefix org: <http://www.knora.org/ontology/shared/organization#>.
@prefix language: <http://www.knora.org/ontology/shared/language#>.
@prefix concept: <http://www.knora.org/ontology/shared/concept#>.
@prefix document: <http://www.knora.org/ontology/shared/document#>.
@prefix text: <http://www.knora.org/ontology/shared/text#>.
@prefix text-expression: <http://www.knora.org/ontology/shared/text-expression#>.
@prefix text-structure: <http://www.knora.org/ontology/shared/text-structure#>.
@prefix textedit: <http://www.knora.org/ontology/shared/text-editing#>.
@prefix infocar: <http://www.knora.org/ontology/shared/information-carrier#>.
@prefix publish: <http://www.knora.org/ontology/shared/publishing#>.
@prefix litera: <http://www.knora.org/ontology/shared/literature#>.
@prefix ling: <http://www.knora.org/ontology/shared/linguistics#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.

@base <http://www.knora.org/ontology/shared/scholarly-editing>.
@prefix scholarly-editing: <http://www.knora.org/ontology/shared/scholarly-editing#>.

<http://www.knora.org/ontology/shared/scholarly-editing>
	a owl:Ontology;
	dct:license <http://creativecommons.org/licenses/by/3.0/>;
	dct:title "An ontology about scientific editing"@en;
	dct:description """Formal description of scientific editing, general subclasses, related classes, and properties."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:contributor "Roberta Padlina, tool coordinator, NIE-INE, Basel University, Switzerland"@en;
	dct:publisher "Basel University, Switzerland"@en;

	knora-base:attachedToProject <http://www.knora.org/ontology/knora-base#DefaultSharedOntologiesProject>.

#
# CLASSES
#

scholarly-editing:EditionScientist
	a owl:Class;
	rdfs:label "edition scientist"@en, "EditionswissenschaftlerIn"@de;
	rdfs:comment """Role of a person practicing edition science."""@en;
	rdfs:subClassOf human:Scientist.

scholarly-editing:Editor
	a owl:Class;
	rdfs:label "scholarly editor"@en, "wissenschaftliche Editor"@de, "éditeur scientifique"@fr;
	rdfs:comment """Role of a person editing a text expression scientifically."""@en; #°
	skos:note """Different from an editor of scientific literature, who is not necessarily a scientist."""@en;
	rdfs:subClassOf textedit:Editor, scholarly-editing:EditionScientist.

scholarly-editing:OrganizationEditor
	a owl:Class;
	rdfs:label "scholarly organization-editor"@en, "wissenschaftliche Organisation-Editor"@de;
	rdfs:comment """Role of an organization editing a text expression scientifically."""@en;
	rdfs:subClassOf org:Creator.

scholarly-editing:Edition
	a owl:Class;
	rdfs:label "scholarly edition"@en, "wissenschaftliche Edition"@de;
	rdfs:comment """Derived text expression edited for scientific purpose."""@en;
	rdfs:subClassOf textedit:DerivedTextExpression , [
		a owl:Restriction; owl:onProperty scholarly-editing:hasEditor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:hasEditorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:Page
	a owl:Class;
	rdfs:label "scholarly edition text page"@en, "wissenschaftliche Editiontextseite"@de;
	rdfs:comment """Page of a scholarly edition."""@en;
	rdfs:subClassOf text-structure:Page, [
		a owl:Restriction; owl:onProperty scholarly-editing:isPageOfEdition; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isPageOfEditionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:EditorialSymbol
	a owl:Class;
	rdfs:label "editorial symbol"@en, "editorisches Zeichen"@de;
	rdfs:comment """Symbol in a scholarly edition indicating a textual property of the original text."""@en;
	rdfs:subClassOf concept:SymbolicEntity, [
		a owl:Restriction; owl:onProperty scholarly-editing:isEditorialSymbolOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isEditorialSymbolOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:PageIndicator
	a owl:Class;
	rdfs:label "page indicator"@en, "Seiteindikator"@de;
	rdfs:comment """Editorial symbol indicating where a document page starts."""@en;
	rdfs:subClassOf scholarly-editing:EditorialSymbol, [
		a owl:Restriction; owl:onProperty scholarly-editing:isPageIndicatorOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isPageIndicatorOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:PageNumberIndicator
	a owl:Class;
	rdfs:label "page number indicator"@en, "Seitenummerindikator"@de;
	rdfs:comment """Page indicator as number."""@en;
	rdfs:subClassOf scholarly-editing:EditorialSymbol, [
		a owl:Restriction; owl:onProperty scholarly-editing:isPageNumberIndicatorOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isPageNumberIndicatorOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:ColumnIndicator
	a owl:Class;
	rdfs:label "column indicator"@en, "Spalteindikator"@de;
	rdfs:comment """Editorial symbol indicating a textual property of the original text."""@en;
	rdfs:subClassOf scholarly-editing:EditorialSymbol, [
		a owl:Restriction; owl:onProperty scholarly-editing:isColumnIndicatorOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isColumnIndicatorOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:DiplomaticTranscription
	a owl:Class;
	rdfs:label "diplomatic transcription"@en, "diplomatische Umschrift"@de;
#	ling:hasSynonym "Lesehilfe"@de;
	rdfs:comment """Transcription of a text with editorial symbols indicating visual textual properties to describe the textual state."""@en;
	skos:note """Mostly done for autographs or juridical text; without interpretation.""";
	rdfs:subClassOf scholarly-editing:Edition, [
		a owl:Restriction; owl:onProperty scholarly-editing:isDiplomaticTranscriptionOfTextOnPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isDiplomaticTranscriptionOfTextOnPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

# textedit:DiplomaticInterpretativeTranscription

scholarly-editing:CriticalEditor
	a owl:Class;
	rdfs:label "critical editor"@en, "kritische Editor"@de;
	rdfs:comment """Role of being a scholar editing a text expression in a critical scientific way."""@en;
	rdfs:subClassOf human:Scientist, human:PersonCreator.

scholarly-editing:CriticalEdition
	a owl:Class;
	rdfs:label "critical text edition"@en, "kritische Textedition"@de;
	rdfs:comment """Edition to which a critical editor added evidence and interpretation of a text creation and historical transmission, using a critical apparatus, allowing readers to assess the editor's choice."""@en;
	rdfs:subClassOf scholarly-editing:Edition, [
		a owl:Restriction; owl:onProperty scholarly-editing:hasCriticalEditor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:hasCriticalEditorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

# textedit:Variorum rdfs:subClassOf textedit:CriticalEdition

scholarly-editing:GeneticEdition
	a owl:Class;
	rdfs:label "genetic edition"@en, "genetische Edition"@de, "édition génétique"@fr, "edizione genetica"@it;
	rdfs:comment """Critical edition accounting for the modifications by the author."""@en;
	rdfs:subClassOf scholarly-editing:CriticalEdition.

scholarly-editing:ConstitutedEdition
	a owl:Class;
	rdfs:label "constituted text edition"@en, "konstituierter Text Edition"@de;
#	ling:hasSynonym "Lesetext"@de;
	rdfs:comment """Plain text expression as endresult of critical editing of another text expression."""@en;
	rdfs:subClassOf scholarly-editing:Edition.

scholarly-editing:EditorialStructure
	a owl:Class;
	rdfs:label "editorial text structure"@en, "editorische Textstruktur"@de;
	rdfs:comment """Scientific text structure depending on editorial purpose, e.g. an editing apparatus."""@en;
	rdfs:subClassOf text-structure:ScientificStructure, [
		a owl:Restriction; owl:onProperty scholarly-editing:hasSiglumReference; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:hasSiglumReferenceValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:SiglumReference
	a owl:Class;
	rdfs:label "siglum reference"@en, "Siglum-Referenz"@de; #, "siglum"@fr, "siglum"@it;
	rdfs:comment """Reference to a siglum as expression identifier."""@en;
	rdfs:subClassOf document:ExpressionReference, [
		a owl:Restriction; owl:onProperty scholarly-editing:hasSiglum; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:hasSiglumValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:Siglum
	a owl:Class;
	rdfs:label "siglum"@en, "Siglum"@de, "siglum"@fr, "siglum"@it;
	rdfs:comment """One or more symbols as identifier of an expression as original or witness in a scholarly edition."""@en;
	rdfs:subClassOf document:ExpressionIdentifier, [
		a owl:Restriction; owl:onProperty scholarly-editing:isSiglumOfEdition; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isSiglumOfEditionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:Apparatus
	a owl:Class;
	rdfs:label "editing apparatus"@en, "Edierungsapparatus"@de, "apparatus editionis"@la;
	rdfs:comment """Editorial compositional structure in a scholarly edition to analyse text along certain criteria."""@en;
	rdfs:subClassOf scholarly-editing:EditorialStructure, text-structure:CompositionalContentStructure, [
		a owl:Restriction; owl:onProperty scholarly-editing:isApparatusOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isApparatusOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:ApparatusEntry
	a owl:Class;
	rdfs:label "editing apparatus entry"@en, "Edierungsapparateintrag"@de; #, "entrée d'apparat éditionel"@fr, "iscrizione del apparato"@it;
	rdfs:comment """Entry of an editing apparatus."""@en;
	rdfs:subClassOf scholarly-editing:EditorialStructure, text-structure:CompositionalContentStructure, [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfApparatus; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfApparatusValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:hasApparatusEnteringDate; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:DoctrinalApparatus
	a owl:Class;
	rdfs:label "doctrinal apparatus"@en, "doktrinal Apparat"@de;
#	ling:hasSynonym textedit:HistoricalApparatus;
	rdfs:comment """Editing apparatus commmenting on the content of a scholarly edition in a historical and doctrinal context."""@en; # info about theory
	rdfs:subClassOf scholarly-editing:Apparatus, [
		a owl:Restriction; owl:onProperty scholarly-editing:isDoctrinalApparatusOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isDoctrinalApparatusOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:DoctrinalApparatusEntry
	a owl:Class;
	rdfs:label "doctrinal apparatus entry"@en, "doktrinaler Apparateintrag"@de; #, "entrée d'apparat doctrine"@fr, "iscrizione del apparato doctrino"@it;
	rdfs:comment """Entry of a doctrinal apparatus."""@en;
	rdfs:subClassOf scholarly-editing:ApparatusEntry, [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfDoctrinalApparatus; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfDoctrinalApparatusValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:ApparatusFontium
	a owl:Class;
	rdfs:label "apparatus fontium"@en, "Apparatus Fontium"@de, "apparatus fontium"@la;
	rdfs:comment """Editing apparatus containing references to one or more citations."""@en;
	rdfs:subClassOf scholarly-editing:Apparatus, [
		a owl:Restriction; owl:onProperty scholarly-editing:isApparatusFontiumOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isApparatusFontiumOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:ApparatusFontiumEntry
	a owl:Class;
	rdfs:label "apparatus fontium entry"@en, "Apparatus Fontium-Eintrag"@de; #, "entrée d'apparat "@fr, "iscrizione del apparato "@it;
	rdfs:comment """Entry of an apparatus fontium."""@en;
	rdfs:subClassOf scholarly-editing:ApparatusEntry, [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfApparatusFontium; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfApparatusFontiumValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:CriticalApparatus
	a owl:Class;
	rdfs:label "critical apparatus"@en, "textkritischer Apparat"@de, "apparat critique"@fr, "apparato critico"@it, "apparatus criticus"@la;
	rdfs:comment """Editing apparatus to a critical text edition providing information of textual versions."""@en;
	rdfs:subClassOf scholarly-editing:Apparatus, [
		a owl:Restriction; owl:onProperty scholarly-editing:isCriticalApparatusOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isCriticalApparatusOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:CriticalApparatusEntry
	a owl:Class;
	rdfs:label "critical apparatus entry"@en, "textkritischer Apparateintrag"@de, "entrée d'apparat critique"@fr, "iscrizione del apparato critico"@it;
	rdfs:comment """Entry of critical apparatus."""@en;
	rdfs:subClassOf scholarly-editing:ApparatusEntry, [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfCriticalApparatus; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfCriticalApparatusValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:StructureApparatus
	a owl:Class;
	rdfs:label "text structure apparatus"@en, "Textstruktur-Apparat"@de;
	rdfs:comment """Editing apparatus to a critical text edition providing information of text structure."""@en;
	rdfs:subClassOf scholarly-editing:Apparatus, [
		a owl:Restriction; owl:onProperty scholarly-editing:isStructureApparatusOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isStructureApparatusOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:StructureApparatusEntry
	a owl:Class;
	rdfs:label "structure apparatus entry"@en, "Textstruktur-Apparateintrag"@de; #, "entrée d'apparat de structure"@fr, "iscrizione del apparato del "@it;
	rdfs:comment """Entry of text structure apparatus."""@en;
	rdfs:subClassOf scholarly-editing:ApparatusEntry, [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfStructureApparatus; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isEntryOfStructureApparatusValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:StructureDescription
	a owl:Class;
	rdfs:label "text structure description"@en, "Textstrukturdeskription"@de;
	rdfs:comment """Description of a text structure in a structure apparatus."""@en;
	rdfs:subClassOf scholarly-editing:EditorialStructure, [
		a owl:Restriction; owl:onProperty scholarly-editing:isStructureDescriptionOfEntry; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isStructureDescriptionOfEntryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:Reading
	a owl:Class;
	rdfs:label "reading"@en, "Lesung"@de, "lezione"@it, "lectio"@la;
	rdfs:comment """Copy of one or more words or phrases of an original or witness."""@en;
	rdfs:subClassOf scholarly-editing:EditorialStructure, text:CopiedText, [
		a owl:Restriction; owl:onProperty scholarly-editing:isReadingOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isReadingOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:Witness
	a owl:Class;
	rdfs:label "witness text"@en, "Textzeuge"@de;
	rdfs:comment """Copy of a text studied in a critical edition, as comparison or proof of existance of the original by looking for features of originality, possibly enabling the original's reconstruction."""@en;
	rdfs:seeAlso <https://www.abc-clio.com/ODLIS/odlis_w.aspx>;
	rdfs:subClassOf text:CopiedText, text-expression:Expression, [
		a owl:Restriction; owl:onProperty scholarly-editing:witnessHasSiglum; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:witnessHasSiglumValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isWitnessOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isWitnessOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:Lemma
	a owl:Class;
	rdfs:label "lemma"@en, "Lemma"@de, "lemma"@fr, "lemma"@it;
	rdfs:comment """Editorial structure in an apparatus entry with a copy of one or more words or phrases referring to its edition text, and as appearing in the original or a witness, or created by an editor, and judged by the editor to be authentic, accurate, and/or authoritative [, and possibly compaired with variants referring to other witnesses]"""@en;
	skos:note """In an apparatus entry a lemma is mostly seperated from a variant or editorial explanation by a right square bracket ']'."""@en;
	rdfs:subClassOf scholarly-editing:EditorialStructure, [
		a owl:Restriction; owl:onProperty scholarly-editing:isLemmaOfCriticalEntry; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isLemmaOfCriticalEntryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isLemmaOfDoctrinalEntry; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isLemmaOfDoctrinalEntryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:lemmaHasReference; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:lemmaHasReferenceValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:lemmaHasVariant; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:lemmaHasVariantValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:lemmaHasComment; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:lemmaHasCommentValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:WitnessedLemma
	a owl:Class;
	rdfs:label "witnessed lemma"@en, "bezeugtes Lemma"@de; #, "lemma"@fr, "lemma"@it;
	rdfs:comment """Lemma with a witness."""@en;
	rdfs:subClassOf scholarly-editing:Lemma, scholarly-editing:Reading.

scholarly-editing:Conjecture
	a owl:Class;
	rdfs:label "conjecture"@en, "Konjektur"@de;
	rdfs:comment """Witness-independent lemma expressly proposed to be of interest but absent in the main text of an edition."""@en;
	rdfs:subClassOf scholarly-editing:Lemma.

scholarly-editing:Emendation
	a owl:Class;
	rdfs:label "emendation"@en, "Emendation"@de, "émendation"@fr, "emendazione"@it;
	rdfs:comment """Conjecture accepted by the editor in the main text of an edition."""@en;
	rdfs:subClassOf scholarly-editing:Conjecture.

scholarly-editing:Variant
	a owl:Class;
	rdfs:label "variant"@en, "Variant"@de, "variante"@fr, "variante"@it;
	rdfs:comment """Reading in a critical apparatus entry, judged by an editor to be unsatisfactory for linguistic and/or stylistic reasons, but of interest for establishing the text, and as such varying from what the editor has judged to be the lemma."""@en;
	skos:note """Absence in a witness of respective part is commented."""@en;
	rdfs:subClassOf scholarly-editing:Reading, [
		a owl:Restriction; owl:onProperty scholarly-editing:isVariantOfEntry; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isVariantOfEntryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:variantHasReference; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:variantHasReferenceValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:variantHasComment; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:variantHasCommentValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

scholarly-editing:EditorialComment
	a owl:Class;
	rdfs:label "editorial comment"@en, "editorische Kommentar"@de; #, ""@fr, ""@it;
	rdfs:comment """Comment in a scholarly edition."""@en;
	rdfs:subClassOf scholarly-editing:EditorialStructure, [
		a owl:Restriction; owl:onProperty scholarly-editing:isCommentOn; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isCommentOnValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isCommentOfEdition; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isCommentOfEditionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isCommentOfEntry; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:isCommentOfEntryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:commentHasReference; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty scholarly-editing:commentHasReferenceValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
