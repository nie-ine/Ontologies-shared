document:hasEncoder
	a owl:ObjectProperty;
	rdfs:label "has encoder"@en, "hat Encoder"@de;
	rdfs:comment "Relating a document to a person who encoded it."@en;
	rdfs:domain document:Document;
#	rdfs:range [rdfs:subClassOf human:Person, [a owl:Restriction; owl:onProperty human:personHasRole; owl:someValuesFrom document:Encoder]];
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint document:Document;
	knora-base:objectClassConstraint human:Person.

document:hasEncoderValue
	a owl:ObjectProperty;
	rdfs:label "has encoder - statement"@en, "hat Encoder - Festlegung"@de;
	rdfs:comment "Relating a document to a reification statement of the relation between the document and a person who encoded it."@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint document:Document;
	knora-base:objectClassConstraint knora-base:LinkValue.

document:isElementOfDocument
	a owl:ObjectProperty;
	rdfs:label "is element of document"@en, "ist Element eines Dokumentes"@de;
	rdfs:comment """Relating a document element to a document it is part of."""@en;
	rdfs:domain document:Element;
	rdfs:range document:Document;
	rdfs:subPropertyOf concept:isPartOf;
	knora-base:subjectClassConstraint document:Element;
