@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix event: <http://www.knora.org/ontology/shared/event#>.
@prefix human: <http://www.knora.org/ontology/shared/human#>.
@prefix language: <http://www.knora.org/ontology/shared/language#>.
@prefix concept: <http://www.knora.org/ontology/shared/concept#>.
@prefix math: <http://www.knora.org/ontology/shared/mathematics#>.
@prefix document: <http://www.knora.org/ontology/shared/document#>.
@prefix text: <http://www.knora.org/ontology/shared/text#>.
@prefix textedit: <http://www.knora.org/ontology/shared/text-editing#>.
@prefix infocar: <http://www.knora.org/ontology/shared/information-carrier#>.
@prefix publish: <http://www.knora.org/ontology/shared/publishing#>.
@prefix litera: <http://www.knora.org/ontology/shared/literature#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.

@base <http://www.knora.org/ontology/shared/text-structure>.
@prefix text-structure: <http://www.knora.org/ontology/shared/text-structure#>.

<http://www.knora.org/ontology/shared/text-structure>
	a owl:Ontology;
	dct:title "An ontology about text structure"@en;
	dct:description """Formal description of text structure, subclasses, related classes, and properties."""@en;
	skos:note """Text and text editing are described in other ontologies"""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:contributor "Roberta Padlina, tool coordinator and software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:publisher "Basel University, Switzerland"@en;

	knora-base:attachedToProject <http://www.knora.org/ontology/knora-base#DefaultSharedOntologiesProject> # <http://rdfh.ch/projects/005B>.

#
# CLASSES
#

text-structure:Structure
	a owl:Class;
	rdfs:label "text structure"@en, "Textstruktur"@de;
	rdfs:comment """Text as structure abstracted from its carrier, e.g. a word, sentence, text line, paragraph, section, verse, strophe."""@en;
	skos:note """Making up a text expression, without necessarily being one itself."""@en;
	rdfs:subClassOf text:Text, document:ContentStructure, [
		a owl:Restriction; owl:onProperty text-structure:isPartOfTextExpression; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isPartOfTextExpressionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:hasIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:hasIdentifierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Identifier
	a owl:Class;
	rdfs:label "text structure identifier"@en, "Textstrukturidentifikator"@de;
	rdfs:comment """Identifier of a text structure."""@en;
	rdfs:subClassOf document:ContentStructureIdentifier. #Â°

text-structure:SyntacticStructure
	a owl:Class;
	rdfs:label "syntactic text structure"@en, "syntaktische Textstruktur"@de;
	rdfs:comment """Text structure depending on natural language-specific syntax, e.g. a word, sentence."""@en;
	rdfs:subClassOf text-structure:Structure.

text-structure:CompositionalStructure
	a owl:Class;
	rdfs:label "compositional text structure"@en, "kompositionelle Textstruktur"@de;
	rdfs:comment """Text structure depending on text composition in a two-dimensional plane, e.g. a line, paragraph, section, page."""@en;
	rdfs:subClassOf text-structure:Structure.

text-structure:ContentStructure
	a owl:Class;
	rdfs:label "content text structure"@en, "inhaltliche Textstruktur"@de;
	rdfs:comment """Text structure depending on text content, e.g. a paragraph, section, prose, citation."""@en;
	rdfs:subClassOf text-structure:Structure. # text:ExpressionPart

text-structure:SentenceStructure
	a owl:Class;
	rdfs:label "sentence structure"@en, "Satzstruktur"@de;
	rdfs:comment """Content structure depending on sentences, e.g. a paragraph, section, prose"""@en;
	rdfs:subClassOf text-structure:ContentStructure.

text-structure:CompositionalContentStructure
	a owl:Class;
	rdfs:label "compositional content text structure"@en, "kompositionelle inhaltliche Textstruktur"@de;
	rdfs:comment """Content text structure depending on text composition, e.g. a paragraph, section."""@en;
	rdfs:subClassOf text-structure:ContentStructure, text-structure:CompositionalStructure.

text-structure:ScientificStructure
	a owl:Class;
	rdfs:label "scientific text structure"@en, "wissenschaftliche Textstruktur"@de;
	rdfs:comment """Content text structure depending on scientific purpose, e.g. a critical apparatus."""@en;
	rdfs:subClassOf text-structure:ContentStructure.

text-structure:ReadabilityStructure
	a owl:Class;
	rdfs:label "text readability structure"@en, "Textlesbarkeitstruktur"@de;
	rdfs:comment """Compositional text structure depending on text readability, e.g. a line, column, page."""@en;
	rdfs:subClassOf text-structure:CompositionalStructure.

text-structure:ProsodicStructure
	a owl:Class;
	rdfs:label "prosodic text structure"@en, "prosodische Textstruktur"@de;
	rdfs:comment """Text structure depending on prosody, i.e. metre as basic rhythmic structure, syllable, and possibly rhyme."""@en;
	rdfs:subClassOf text-structure:CompositionalContentStructure.

text-structure:Word
	a owl:Class;
	rdfs:label "word"@en, "Wort"@de;
	rdfs:comment """Smallest independent syntactic and semantic unit composed of symbols."""@en;
	skos:note """A morpheme is not independent."""@en;
	rdfs:subClassOf text-structure:SyntacticStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isWordOfSentence; owl:allValuesFrom text-structure:Sentence], [
		a owl:Restriction; owl:onProperty text-structure:isWordOfSentence; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isWordOfSentenceValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty text-structure:isWordOfLine; owl:allValuesFrom text-structure:Line], [
		a owl:Restriction; owl:onProperty text-structure:isWordOfLine; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isWordOfLineValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty text-structure:isWordOfVerse; owl:allValuesFrom text-structure:Verse], [
		a owl:Restriction; owl:onProperty text-structure:isWordOfVerse; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isWordOfVerseValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Sentence
	a owl:Class;
	rdfs:label "sentence"@en, "Satz"@de;
	rdfs:comment """Syntactic structure unit comprising one or more linked words, typically a subject and predicate."""@en;
	rdfs:subClassOf text-structure:SyntacticStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isSentenceOfParagraph; owl:allValuesFrom text-structure:Paragraph], [
		a owl:Restriction; owl:onProperty text-structure:isSentenceOfParagraph; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isSentenceOfParagraphValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty text-structure:isSentenceOfProse; owl:allValuesFrom text-structure:Prose], [
		a owl:Restriction; owl:onProperty text-structure:isSentenceOfProse; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isSentenceOfProseValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

# text-structure:WrittenSentence: orthographic element: one or more words starting with a capital letter and ending in a full stop or period, question mark, or exclamation mark

text-structure:Prose
	a owl:Class;
	rdfs:label "prose"@en, "Prosa"@de;
	rdfs:comment """Syntactic structure with a sentence as unit, in a natural flow of speech."""@en;
	rdfs:subClassOf text-structure:SentenceStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isProseOfExpression; owl:allValuesFrom text:Text], [
		a owl:Restriction; owl:onProperty text-structure:isProseOfExpression; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isProseOfExpressionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Paragraph
	a owl:Class;
	rdfs:label "paragraph"@en, "Paragraf"@de;
	rdfs:comment """Composition of sentences ending with a line break."""@en; #*
	rdfs:subClassOf text-structure:SentenceStructure, text-structure:CompositionalContentStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isParagraphOfSection; owl:allValuesFrom text:Section], [
		a owl:Restriction; owl:onProperty text-structure:isParagraphOfSection; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isParagraphOfSectionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:paragraphHasTitle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:paragraphHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Section
	a owl:Class;
	rdfs:label "text section"@en, "Textabschnitt"@de;
	rdfs:comment """Composition of paragraphs, mostly about a certain topic."""@en;
	rdfs:subClassOf text-structure:SentenceStructure, text-structure:CompositionalContentStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isSectionOfExpression; owl:allValuesFrom text:Section], [
		a owl:Restriction; owl:onProperty text-structure:isSectionOfExpression; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isSectionOfExpressionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:sectionHasTitle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:sectionHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Line
	a owl:Class;
	rdfs:label "text line"@en, "Textzeile"@de;
	rdfs:comment """Text structured as a line."""@en;
	rdfs:subClassOf text-structure:ReadabilityStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isLineOfColumn; owl:allValuesFrom document:Column], [
		a owl:Restriction; owl:onProperty text-structure:isLineOfColumn; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isLineOfColumnValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:lineHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Column
	a owl:Class;
	rdfs:label "text column"@en, "Textspalte"@de;
	rdfs:comment """Text structured as a column."""@en;
	rdfs:subClassOf text-structure:ReadabilityStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isTextOfColumn; owl:allValuesFrom document:Column], [
		a owl:Restriction; owl:onProperty text-structure:isTextOfColumn; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isTextOfColumnValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty text-structure:isColumnOfPage; owl:allValuesFrom document:Page], [
		a owl:Restriction; owl:onProperty text-structure:isColumnOfPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isColumnOfPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Page
	a owl:Class;
	rdfs:label "text page"@en, "Textseite"@de;
	rdfs:comment """Text structured as a page."""@en;
	rdfs:subClassOf text-structure:ReadabilityStructure, [
		a owl:Restriction; owl:onProperty text-structure:isTextOfPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isTextOfPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isPageOfExpression; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isPageOfExpressionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Table #* tableHasTitle, tableHasField, tableHasRow, tableHasColumn
	a owl:Class;
	rdfs:label "text table"@en, "Texttabelle"@de;
	rdfs:comment """Two-dimensional structure composed of one or more fields containing text, and  being part of a vertical column and a horizontal row, both with own identifier."""@en;
	rdfs:subClassOf text-structure:CompositionalContentStructure, document:Table.

text-structure:Verse
	a owl:Class;
	rdfs:label "verse"@en, "Vers"@de;
	rdfs:comment """Text line with prosody."""@en; # recurring metrical unit
	rdfs:subClassOf text-structure:ProsodicStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isVerseOfStrophe; owl:allValuesFrom text-structure:Strophe], [ 
		a owl:Restriction; owl:onProperty text-structure:isVerseOfStrophe; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isVerseOfStropheValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isVerseOfHalfStrophe; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isVerseOfHalfStropheValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
 		a owl:Restriction; owl:onProperty text-structure:verseHasIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:verseHasIdentifierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:verseHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:verseHasNumberValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:VerseIdentifier
	a owl:Class;
	rdfs:label "verse identifier"@en, "Versidentifikator"@de;
	rdfs:comment """Identifier of a verse."""@en;
	rdfs:subClassOf text-structure:Identifier.

text-structure:VerseNumber
	a owl:Class;
	rdfs:label "verse number"@en, "Versnummer"@de;
	rdfs:comment """Number of a verse."""@en;
	rdfs:subClassOf text-structure:VerseIdentifier, math:NaturalNumber.

text-structure:HalfVerse
	a owl:Class;
	rdfs:label "half-verse"@en, "Halbvers"@de;
#	ling:hasSynonym "hemistich"@en, "Hemistich"@de;
	rdfs:comment """Half a verse."""@en;
	rdfs:subClassOf text-structure:ProsodicStructure, [
		a owl:Restriction; owl:onProperty text-structure:isHalfOfVerse; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isHalfOfVerseValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:Strophe
	a owl:Class;
	rdfs:label "strophe"@en, "Strophe"@de;
#	ling:hasSynonym "stanza"@en, "Stanze"@de;
	rdfs:comment """Group of verses."""@en;
	skos:note """The lyriks of a song can be structured as a poem with strophes."""@en;
	rdfs:subClassOf text-structure:ProsodicStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isStropheOfSection; owl:allValuesFrom text:Text], [
		a owl:Restriction; owl:onProperty text-structure:isStropheOfSection; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isStropheOfSectionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isStropheOfPoem; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isStropheOfPoemValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isStropheOfNovel; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isStropheOfNovelValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:stropheHasIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:stropheHasIdentifierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:stropheHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:StropheIdentifier
	a owl:Class;
	rdfs:label "strophe identifier"@en, "Stropheidentifikator"@de;
	rdfs:comment """Identifier of a strophe."""@en;
	rdfs:subClassOf text-structure:Identifier.

text-structure:HalfStrophe # NOT hemistich! = half-verse # :Distich can be SCO :HalfStrophe
	a owl:Class;
	rdfs:label "half-strophe"@en, "Halbstrophe"@de;
	rdfs:comment """Half a strophe."""@en;
	rdfs:subClassOf text-structure:ProsodicStructure, [
		a owl:Restriction; owl:onProperty text-structure:isHalfOfStrophe; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isHalfOfStropheValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:VerseSection
	a owl:Class;
	rdfs:label "verse section"@en, "Versabschnitt"@de;
#	ling:hasSynonym "canto"@en, "Canto"@de, "chant"@fr, "canto"@it;
	rdfs:comment """Group of strophes."""@en;
	rdfs:subClassOf text-structure:ProsodicStructure, [
#		a owl:Restriction; owl:onProperty text-structure:isVerseSectionOfPoem; owl:allValuesFrom text:Text], [
		a owl:Restriction; owl:onProperty text-structure:isVerseSectionOfPoem; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isVerseSectionOfPoemValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isVerseSectionOfVerseNovel; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isVerseSectionOfVerseNovelValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:verseSectionHasTitle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:verseSectionHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:FreeVerse
	a owl:Class;
	rdfs:label "free verse"@en, "freie Vers"@de;
	rdfs:comment """Verse without a fixed prosody."""@en; #Â°
	rdfs:subClassOf text-structure:ProsodicStructure.

# text-structure:FixedVerse and SC
# different meters

text-structure:Rhyme
	a owl:Class;
	rdfs:label "rhyme"@en, "Reim"@de;
	rdfs:comment """Words or phrases with whole or end corresponding sound, especially when involving the last stressed vowel and all succeeding sounds."""@en;
	rdfs:subClassOf text-structure:ContentStructure.

text-structure:RhymingVerse
	a owl:Class;
	rdfs:label "rhyming verse"@en, "gereimter Vers"@de;
	rdfs:comment """Verse with a rhyme."""@en;
	rdfs:subClassOf text-structure:Verse, text-structure:Rhyme.

text-structure:NonRhymingVerse
	a owl:Class;
	rdfs:label "non-rhyming verse"@en, "nichtgereimter Vers"@de;
	rdfs:comment """Verse without a rhyme."""@en;
	rdfs:subClassOf text-structure:Verse.

text-structure:RhythmicProse
	a owl:Class;
	rdfs:label "rhythmic prose"@en, "rhythmische Prosa"@de;
	rdfs:comment """Prose structure with a meter."""@en;
	rdfs:subClassOf text-structure:Prose, text-structure:ProsodicStructure.

text-structure:RhymingRhythmicProse
	a owl:Class;
	rdfs:label "rhyming rhythmic prose"@en, "gereimte rhythmische Prosa"@de;
	rdfs:comment """Rhythmic prose with a rhyme."""@en;
	rdfs:subClassOf text-structure:RhythmicProse, text-structure:Rhyme.

text-structure:NonRhymingRhythmicProse
	a owl:Class;
	rdfs:label "non-rhyming rhythmic prose"@en, "nichtgereimte rhythmische Prosa"@de;
	rdfs:comment """Rhythmic prose without a rhyme."""@en;
	rdfs:subClassOf text-structure:RhythmicProse.

text-structure:Dialogue
	a owl:Class;
	rdfs:label "dialogue"@en, "Dialog"@de;
	rdfs:comment """Text with dialogue structure."""@en;
	rdfs:subClassOf text-structure:CompositionalContentStructure.

text-structure:Note
	a owl:Class;
	rdfs:label "note structure"@en, "Notiz-Struktur"@de;
	rdfs:comment """Text with note structure, not necessarily having a unit such as a sentence or being syntactically correct; e.g. a first quick registration of thought, or a remark related to another text."""@en;
	rdfs:subClassOf text-structure:CompositionalContentStructure, [
		a owl:Restriction; owl:onProperty text-structure:isNoteStructureOfExpression; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isNoteStructureOfExpressionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:MarginalNote
	a owl:Class;
	rdfs:label "marginal note"@en, "Marginale Notiz"@de;
	rdfs:comment """Note written besides a text in the margin of a carrier."""@en;
	rdfs:subClassOf text-structure:Note, text:WrittenText, document:Marginal.

text-structure:InterlinearNote
	a owl:Class;
	rdfs:label "interlinear note"@en, "interlineare Notiz"@de;
	rdfs:comment """Note written between text lines."""@en;
	rdfs:subClassOf text-structure:Note, text:WrittenText.

text-structure:Gloss
	a owl:Class;
	rdfs:label "gloss"@en, "Glosse"@de;
	rdfs:comment """Explanatory note, e.g. interlinear or marginal."""@en;
	rdfs:subClassOf text-structure:Note, text:WrittenText, [
		a owl:Restriction; owl:onProperty text-structure:isGlossOfExpression; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text-structure:isGlossOfExpressionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text-structure:MarginalGloss
	a owl:Class;
	rdfs:label "marginal gloss"@en, "marginale Glosse"@de;
	rdfs:comment """Gloss as a marginal note."""@en;
	rdfs:subClassOf text-structure:Gloss, text-structure:MarginalNote.

text-structure:InterlinearGloss
	a owl:Class;
	rdfs:label "interlinear gloss"@en, "interlineare Glosse"@de;
	rdfs:comment """Gloss as an interlinear note."""@en;
	rdfs:subClassOf text-structure:Gloss, text-structure:InterlinearNote.

text-structure:Footnote
	a owl:Class;
	rdfs:label "footnote structure"@en, "Fussnotestruktur"@de;
	rdfs:comment """Note on information on a page at the end of that page, seperate from the main content."""@en;
	rdfs:subClassOf text-structure:Note, [
		a owl:Restriction; owl:onProperty text-structure:footnoteHasIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
